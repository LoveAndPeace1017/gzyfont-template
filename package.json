  "scripts": {
    "start": "cross-env webpack-dev-server --config client/webpack.config.js --mode=development",
    "start:mobile": "cross-env webpack-dev-server --config mobile_client/webpack.config.js --mode=development",
    "build": "webpack --config client/webpack.config.js --mode=production",
    "build:mobile": "webpack --config mobile_client/webpack.config.js --mode=production",
    "build:stats": "cross-env webpack --config client/webpack.config.js --mode=production --profile --json > stats.json",
