    "doc": "cross-env BUILD_ENV=docDev styleguidist server --config client/styleguide.config.js",
    "build:doc": "cross-env  BUILD_ENV=docProd  styleguidist build --config client/styleguide.config.js",
    "analyz": "NODE_ENV=production npm_config_report=true npm run build"
  },
